{
  "ts-node": {
    "compilerOptions": {
      "target": "ES2016",
      "module": "CommonJS",
      "declaration": false,
    }
  },
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "declaration": true,
    "strictNullChecks": false,
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true
  }
}
